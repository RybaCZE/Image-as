{
  "manifest_version": 2,
  "name": "Image as",
  "version": "1.0",
  "description": "Allows you to download images as an PNG, JPEG or Webp",
  "permissions": ["contextMenus", "activeTab"],
  "background": {
    "scripts": ["script.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "96": "icon/ico.png"
  }
}
